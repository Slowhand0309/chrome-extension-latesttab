{
  "name": "Latest tabs switcher",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Switching latest tabs.",
  "icons": {
    "48": "icon_48.png"
  },
  "browser_action": {
    "default_icon": "icon_32.png",
    "browser_style": true,
    "default_title": "Latest tabs switcher",
    "default_popup": "latesttab.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "activate-latest-tab": {
      "suggested_key": {
        "windows": "Alt+Tab",
        "mac": "Alt+Tab"
      },
      "description": "Activate latest tab"
    }
  }
}